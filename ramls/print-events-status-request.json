{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Print Events Request",
  "type": "object",
  "properties": {
    "requestIds": {
      "description": "List of request IDs",
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "pattern": "^[a-fA-F0-9]{8}-[a-fA-F0-9]{4}-[1-5][a-fA-F0-9]{3}-[89abAB][a-fA-F0-9]{3}-[a-fA-F0-9]{12}$"
      }
    }
  }
}

